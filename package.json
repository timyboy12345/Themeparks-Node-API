{
  "name": "themeparks-node-api",
  "version": "0.4.14",
  "description": "An API which can retrieve theme park wait times.",
  "author": "Tim Arendsen",
  "private": false,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "dev": "npm run start:dev",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest --verbose --detectOpenHandles --forceExit --testTimeout 15000",
    "test:watch": "jest --watch --testTimeout 15000",
    "test:cov": "jest --coverage --testTimeout 15000",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/axios": "^2.0.0",
    "@nestjs/common": "^9.3.12",
    "@nestjs/config": "^2.3.1",
    "@nestjs/core": "^9.3.12",
    "@nestjs/jwt": "^10.0.2",
    "@nestjs/passport": "^9.0.3",
    "@nestjs/platform-express": "^9.3.12",
    "@nestjs/schedule": "^0.4.3",
    "@nestjs/swagger": "^6.2.1",
    "@nestjs/typeorm": "^7.1.5",
    "@sentry/node": "^6.19.7",
    "@sentry/tracing": "^6.19.7",
    "@types/unzipper": "^0.10.5",
    "axios": "^1.3.4",
    "cache-manager": "^5.2.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "graphql": "^15.8.0",
    "graphql-request": "^3.7.0",
    "graphql.macro": "^1.4.2",
    "js-base64": "^3.7.5",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.42",
    "mysql2": "^2.3.3",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.8.0",
    "swagger-ui-express": "^4.6.2",
    "typeorm": "^0.3.17",
    "unzipper": "^0.10.11",
    "yauzl": "^2.10.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.3.0",
    "@nestjs/schematics": "^9.0.4",
    "@nestjs/testing": "^9.3.12",
    "@types/cache-manager": "^4.0.2",
    "@types/cron": "^2.0.1",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.0.0",
    "@types/node": "^18.15.10",
    "@types/passport-jwt": "^3.0.8",
    "@types/passport-local": "^1.0.35",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^3.4.1",
    "jest": "^29.0.0",
    "prettier": "^2.8.7",
    "supertest": "^6.3.3",
    "ts-jest": "^29.0.0",
    "ts-loader": "^8.4.0",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.14.2",
    "typescript": "^4.9.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
